{
  "compilerOptions": {

    "target": "es5",                          
    "module": "commonjs",
    "resolveJsonModule": true,
    "esModuleInterop": true, 
    "emitDecoratorMetadata": true,                
    "experimentalDecorators": true,
    "outDir": "typeScriptConvertedFiles",
    
    "types": [
        "jasmine",
        "jasminewd2",
        "node",
        "protractor"
      ]
    }, 
  
    "exclude": [
        "node_modules",
        "typeScriptConvertedFiles",
        "results",
        "logs"
    ] 
}
